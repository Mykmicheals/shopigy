{
    "version": 2,
    "builds": [
        {
            "src": "**/*.ts",
            "use": "@vercel/node-server",
            "config": {
                "distDir": "dist",
                "env": {
                    "TS_NODE_PROJECT": "tsconfig.json"
                }
            }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "api/$1"
        }
    ]
}